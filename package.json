{
  "name": "bulma-dashboard",
  "version": "1.0.0",
  "description": "Personal dashboard built using the Bulma CSS framework",
  "main": "_scss/bulma.scss",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url _scss/bulma.scss css/bulma.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LewTrn/dashboard.git"
  },
  "author": "Lewis Tran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LewTrn/dashboard/issues"
  },
  "homepage": "https://github.com/LewTrn/dashboard#readme",
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "bulma": "^0.7.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "bulma-checkradio": "^1.1.1"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ]
  }
}
